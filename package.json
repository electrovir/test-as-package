{
    "name": "test-as-package",
    "version": "0.0.3",
    "description": "Test a repository as an npm package applied to internal test repos.",
    "keywords": [
        "test",
        "package",
        "repo",
        "npm",
        "internal"
    ],
    "homepage": "https://github.com/electrovir/test-as-package",
    "bugs": {
        "url": "https://github.com/electrovir/test-as-package/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/test-as-package"
    },
    "license": "(MIT or CC0 1.0)",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/api/api.js",
    "types": "dist/api/api.d.ts",
    "bin": "dist/cli/cli.js",
    "scripts": {
        "compile": "rm -rf dist && virmator compile",
        "docs:check": "virmator code-in-markdown check",
        "docs:update": "virmator code-in-markdown",
        "format": "virmator format",
        "publish": "virmator publish \"npm run compile && npm run test:all\"",
        "test": "virmator test",
        "test:all": "npm run test:types && npm test && npm run test:spelling && npm run test:format && npm run docs:check",
        "test:coverage": "npm run test coverage",
        "test:docs": "virmator code-in-markdown check",
        "test:format": "virmator format check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/common": "^13.2.2",
        "@augment-vir/node-js": "^13.2.2",
        "@types/node": "^18.15.11"
    },
    "devDependencies": {
        "@augment-vir/chai": "^13.2.2",
        "@electrovir/nyc": "^15.1.0-fix0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "chai": "^4.3.7",
        "cspell": "^6.31.1",
        "istanbul-smart-text-reporter": "^1.1.1",
        "markdown-code-example-inserter": "^0.3.0",
        "mocha": "^10.2.0",
        "mocha-spec-reporter-with-file-names": "^0.0.3",
        "prettier": "^2.8.7",
        "prettier-plugin-interpolated-html-tags": "^0.0.3",
        "prettier-plugin-jsdoc": "^0.4.2",
        "prettier-plugin-multiline-arrays": "^1.1.3",
        "prettier-plugin-organize-imports": "^3.2.2",
        "prettier-plugin-packagejson": "^2.4.3",
        "prettier-plugin-sort-json": "^1.0.0",
        "prettier-plugin-toml": "^0.3.1",
        "test-established-expectations": "^0.3.0",
        "ts-node": "^10.9.1",
        "type-fest": "^3.7.2",
        "typescript": "^5.0.3",
        "virmator": "^6.3.1"
    }
}
